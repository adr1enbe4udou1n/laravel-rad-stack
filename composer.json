{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "barryvdh/laravel-elfinder": "^0.4.7",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "genl/matice": "^1.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "http-interop/http-factory-guzzle": "^1.0",
        "inertiajs/inertia-laravel": "^0.4.2",
        "itsgoingd/clockwork": "^5.0",
        "laravel/fortify": "^1.7",
        "laravel/framework": "^8.49",
        "laravel/sanctum": "^2.11",
        "laravel/scout": "^9.2",
        "laravel/tinker": "^2.5",
        "league/glide-symfony": "^1.0",
        "livewire/livewire": "^2.4",
        "maatwebsite/excel": "^3.1",
        "meilisearch/meilisearch-php": "^0.18.3",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-enum": "^2.5",
        "spatie/laravel-medialibrary": "^9.6",
        "spatie/laravel-query-builder": "^3.4",
        "spatie/laravel-route-attributes": "^1.4",
        "spatie/laravel-sluggable": "^3.1",
        "spatie/laravel-tags": "^4.0",
        "spatie/laravel-translatable": "^5.0",
        "tightenco/ziggy": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^0.7.6",
        "pestphp/pest-plugin-laravel": "^1.0",
        "phpro/grumphp-shim": "^1.3",
        "phpunit/phpunit": "^9.3.3",
        "spatie/phpunit-watcher": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "format": [
            "php-cs-fixer fix --ansi"
        ],
        "analyse": [
            "phpstan analyse --ansi"
        ],
        "test": [
            "@php artisan test --coverage --min=90"
        ],
        "test:watch": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit-watcher watch"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
